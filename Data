<script type="text/javascript">
    // Add the class of the current content-container to to body
    //this is used for proper message-alignment as the messages are outside of the content-container
    document.getElementsByTagName('body').item(0).classList.remove('content-layout-nojs');

    if(document.getElementsByClassName('content-layout-limited-width').length > 0) {
        document.getElementsByTagName('body').item(0).classList.add('content-layout-limited-width-jsset');
    } else {
        document.getElementsByTagName('body').item(0).classList.add('content-layout-default-width-jsset');
    }

</script>

